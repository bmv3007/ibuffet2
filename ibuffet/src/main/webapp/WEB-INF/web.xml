<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Controller</servlet-name>
  	<servlet-class>com.mycompany.ibuffets.web.Controller</servlet-class>
   </servlet>
  <servlet>
  	<servlet-name>Controller2</servlet-name>
  	<display-name>Controller2</display-name>
  	<description></description>
  	<servlet-class>com.mycompany.ibuffet.web.Controller2</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Controller</servlet-name>
  	<url-pattern>/Controller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Controller2</servlet-name>
  	<url-pattern>/Controller2</url-pattern>
  </servlet-mapping>

  
</web-app>
